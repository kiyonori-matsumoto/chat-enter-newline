{
  "manifest_version": 3,
  "name": "Gemini Enter for Newline",
  "version": "1.0.0",
  "description": "Enterで改行、Cmd+Enter/Ctrl+Enterで送信",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/app*"],
      "js": ["content-scripts/index.js"],
      "run_at": "document_start"
    }
  ]
}
